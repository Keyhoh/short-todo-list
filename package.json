{
  "name": "short-todo-list",
  "version": "1.0.0",
  "description": "Todo list that can be operated with shortcut keys.",
  "main": "index.js",
  "scripts": {
    "start": "npm run webpack && npm run electron",
    "build:win": "npm run webpack && npm run electron:win",
    "electron": "electron .",
    "electron:win": "node config/electron/windows.js",
    "webpack": "webpack --mode production --config config/webpack.config.js",
    "test": "mocha './test/**/*.js' --require @babel/register",
    "test:watch": "mocha './test/**/*.js' --require @babel/register --watch",
    "test:operation": "mocha './test/operation/**/*.js' --require @babel/register",
    "test:operation:watch": "mocha './test/operation/**/*.js' --require @babel/register --watch",
    "test:app": "mocha './test/app/**/*.js' --require @babel/register",
    "test:app:watch": "mocha './test/app/**/*.js' --require @babel/register --watch",
    "test:app:target": "mocha --grep ^target: ./test/app/**/*.js --require @babel/register"
  },
  "keywords": [],
  "author": "iheyhoh",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.4",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.1",
    "electron": "^7.1.3",
    "electron-builder": "^21.2.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15",
    "mocha": "^10.2.0",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sass-loader": "^8.0.0",
    "spectron": "^9.0.0",
    "style-loader": "^1.0.1",
    "uuid-random": "^1.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {}
}
