{
  "name": "short-todo-list",
  "version": "1.0.0",
  "description": "Todo list that can be operated with shortcut keys.",
  "main": "index.js",
  "scripts": {
    "start": "npm run webpack && electron .",
    "build:win": "npm run webpack && npm run electron:win",
    "electron:win": "node config/electron/windows.js",
    "webpack": "webpack --mode production --config config/webpack.config.js",
    "test": "mocha './test/**/*.js'",
    "test:watch": "mocha './test/**/*.js' --watch",
    "test:components": "mocha './test/components/**/*.js' --require @babel/register",
    "test:operation": "mocha './test/operation/**/*.js' --require @babel/register",
    "test:operation:watch": "mocha './test/operation/**/*.js' --watch --require @babel/register"
  },
  "keywords": [],
  "author": "iheyhoh",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.0",
    "babel-loader": "^8.0.6",
    "electron": "^7.1.1",
    "electron-builder": "^21.2.0",
    "fs-extra": "^8.1.0",
    "jsdom": "^15.2.1",
    "lodash": "^4.17.15",
    "mocha": "^6.2.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "spectron": "^9.0.0",
    "uuid-random": "^1.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {}
}
